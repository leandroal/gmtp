@inproceedings{Ying2005,
  author    = {Ying Xu and Wenjun Wu},
  title     = {{S}imulations of {TCP} {C}ongestion {C}ontrol {M}echanism {M}odel
with {L}arge {B}uffer {S}ize},
  booktitle = {2001 {I}nternational {C}onference on {C}omputer {N}etworks and
{M}obile {C}omputing},
  volume    = {1},
  year      = {2001},
  pages     = {105--110},
  abstract  = {The Transmission Control Protocol (TCP) is the main protocol of
  the transport layer in today's Internet. Many works have studied the
performance
  of TCP and researchers are trying to analytically characterize the throughput
of
  TCP's congestion control mechanism. We summarize previous works and present
the
  simulation result with large buffer size and give a qualitative analysis of
the
  result. Our conclusion is that the RTT is not increased by a linear amount,
but
  by a quadric amount when the buffer size of the bottleneck is large enough.
Our
  goal is to estimate the average queue length of the buffer size and develop a
  TCP model based on RTT and the average queue length}
}

