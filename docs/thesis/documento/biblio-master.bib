@inproceedings{CONG:Nivor05:Video-Exper:inp,
	abstract = {The Datagram Congestion Control Protocol (DCCP) is being
proposed by IETF as a transport protocol to support multimedia traffic. Two
congestion controls mechanisms have been proposed so far for DCCP, namely
TCP-Like and TCP-Friendly Rate Control (TFRC). In this paper, we experimentally
investigate the DCCP performance with its two congestion control algorithms for
two kinds of video streaming applications, those using XVID and MPEG codecs. The
first stream want to make quick use of any available bandwidth, while the second
prefer to minimize abrupt changes in the sending rate. Our results confirm these
preferences for each stream regard to DCCP congestion controls.},
	author = {Nivor, Frederic  },
	booktitle = {International {C}onference {O}n {E}merging {N}etworking
{E}xperiments {A}nd {T}echnologies ({C}o{NEXT}'05)},
	citeulike-article-id = {1413421},
	doi = {http://doi.acm.org/10.1145/1095921.1095976},
	keywords = {bibtex-import, ccid, congestion, control, dccp, mpeg,
streaming, tfrc, xvid},
	organization = {ACM},
	pages = {272--273},
	posted-at = {2007-06-26 14:07:24},
	priority = {0},
	title = {{E}xperimental {S}tudy of {DCCP} for {M}ultimedia
{A}pplications},
	year = {2005}
}

