#!/usr/bin/gnuplot -persist

set terminal png; set output "ssthresh.png"; # uncomment to produce PNG

set title  "congestion response"

set style data linespoints

set xlabel "time (seconds)"
set ylabel "packets"

set key below	

#set xrange [0:5]
#set logscale y

plot	"/tmp/dccp_probe.out" using 1:9 title "cwnd", \
	"/tmp/dccp_probe.out" using 1:10 title "pipe",\
	"/tmp/dccp_probe.out" using 1:($11 >= 4294967295.0 ? 0 : $11) title "ssthresh"
