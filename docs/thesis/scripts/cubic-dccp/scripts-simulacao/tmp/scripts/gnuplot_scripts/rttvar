#!/usr/bin/gnuplot -persist

set terminal png; set output "rttvar.png"; # uncomment to produce PNG

set title  "Clamped mean deviation of RTT samples

set style data linespoints

set xlabel "time (seconds)"
set ylabel "milliseconds"

plot	"/tmp/dccp_probe.out" using 1:7 title "mdev", \
	"/tmp/dccp_probe.out" using 1:8 title "rttvar" 
